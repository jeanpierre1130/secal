<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <application>
       
        <locale-config>
            <default-locale>es_PE</default-locale>            
            <supported-locale>es_ES</supported-locale>
            <supported-locale>en_US</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>compile</base-name>
            <var>compile</var>
        </resource-bundle> 
    </application>
    
    <navigation-rule> 
        <navigation-case>
            <from-outcome>pagina_sesion</from-outcome>
            <to-view-id>/ui/ServiceLogin.xhtml</to-view-id>
        </navigation-case> 
<!--        
        <navigation-case>
            <from-outcome>pagina_principal</from-outcome>
            <to-view-id>WEB-INF/template_layout.xhtml</to-view-id>
        </navigation-case> -->
    </navigation-rule>
</faces-config>